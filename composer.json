{
    "name": "platform/testmodule",
    "description": "Test module for Platform.",
    "license": "proprietary",
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Platform\\Test\\Providers\\ModuleServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Platform\\Test\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
